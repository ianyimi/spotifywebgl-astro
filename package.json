{
  "name": "spotify-webgl-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "db-generate": "drizzle-kit generate:sqlite --config drizzle.config.ts",
    "db-pull": "drizzle-kit introspect:sqlite --config drizzle.config.ts",
    "db-push": "drizzle-kit push:sqlite --config drizzle.config.ts",
    "db-check": "drizzle-kit check:sqlite --config drizzle.config.ts",
    "db-drop": "drizzle-kit drop --config drizzle.config.ts",
    "db-studio": "drizzle-kit studio --host 127.0.0.1 --port 3005",
    "db-migrate": "tsx src/db/migrate"
  },
  "dependencies": {
    "@astrojs/react": "^3.0.3",
