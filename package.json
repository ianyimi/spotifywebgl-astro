    "@legendapp/state": "^2.0.1",
    "@libsql/client": "^0.3.5",
    "astro": "^3.3.2",
    "astro-compress": "^2.0.15",
    "auth-astro": "^3.0.1",
    "cookie": "^0.5.0",
    "drizzle-orm": "^0.28.6",
    "drizzle-zod": "^0.5.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "spotify-web-api-node": "^5.0.2",
